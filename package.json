{
  "name": "moeum-eslint-prettier-toolkit",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "publish:cli": "turbo run deploy --filter=@moeum/eslint-prettier-cli",
    "publish:base": "turbo run deploy --filter=@moeum/eslint-config-base",
    "publish:react": "turbo run deploy --filter=@moeum/eslint-react",
    "publish:next": "turbo run deploy --filter=@moeum/eslint-next",
    "publish:rn-expo": "turbo run deploy --filter=@moeum/eslint-rn-expo",
    "publish:nest": "turbo run deploy --filter=@moeum/eslint-config-nest"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "^2.3.3",
    "typescript": "5.5.4"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
